@{
    ViewData["Title"] = "Register Car";
}


<h1>Register new car here</h1>

<div class="form">

    <div class="form-group row">
        <label for="txtMake" class="col-4 col-form-label">Make</label>
        <div class="col-8">
            <input id="txtMake" name="txtMake" type="text" class="form-control">
        </div>
</div>
   <div class="form-group row">
        <label for="txtModel" class="col-4 col-form-label">Model</label>
        <div class="col-8">
            <input id="txtModel" name="txtModel" type="text" class="form-control">
        </div>
</div>
   <div class="form-group row">
        <label for="txtYear" class="col-4 col-form-label">Year</label>
        <div class="col-8">
            <input id="txtYear" name="txtYear" type="text" class="form-control">
        </div>
</div>
   <div class="form-group row">
        <label for="txtCyls" class="col-4 col-form-label">Cyls</label>
        <div class="col-8">
            <input id="txtCyls" name="txtCyls" type="text" class="form-control">
        </div>
</div>
 <div class="form-group row">
        <label for="txtDescription" class="col-4 col-form-label">Description</label>
        <div class="col-8">
            <input id="txtDescription" name="txtDescription" type="text" class="form-control">
        </div>
</div>
 <div class="form-group row">
        <label for="txtDailyPrice" class="col-4 col-form-label">Daily Price</label>
        <div class="col-8">
            <input id="txtDailyPrice" name="txtDailyPrice" type="text" class="form-control">
        </div>
</div>
 <div class="form-group row">
        <label for="txtImageURL" class="col-4 col-form-label">ImageURL</label>
        <div class="col-8">
            <input id="txtImageURL" name="txtImageURL" type="text" class="form-control">
        </div>
</div>





